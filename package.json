{
    "name": "shipping-only-dashboard",
    "description": "client facing dashboard for user's to interact with their shipping-only platform",
    "version": "0.0.1",
    "homepage": "https://github.com/RaiseEngineering/dashboard",
    "repository": {
        "type": "git",
        "url": "https://github.com/RaiseEngineering/dashboard"
    },
    "bugs": {
        "url": "https://github.com/RaiseEngineering/dashboard/issues"
    },
    "main": "public/index.html",
    "husky": {
        "hooks": {
            "pre-commit": "npm run eslint",
            "pre-push": "npm test"
        }
    },
    "scripts": {
        "start": "webpack-dev-server",
        "build": "webpack",
        "test": "NO_BUNDLE='true' cross-env BABEL_ENV=test karma start",
        "watch": "webpack -w -d",
        "eslint": "eslint app/scripts",
        "prettier": "prettier --check --loglevel=warn ./app/scripts/**/*.js"
    },
    "nyc": {
        "include": [
            "src/**/*.(js|vue)"
        ],
        "instrument": false,
        "sourceMap": false
    },
